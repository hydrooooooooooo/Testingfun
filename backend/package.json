{
  "name": "marketplace-scraper-pro-backend",
  "version": "1.0.0",
  "description": "Backend for Marketplace Scraper Pro - APIFY and Stripe integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npm run db:migrate && ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "db:migrate": "knex migrate:latest --knexfile src/config/knexfile.ts",
    "db:rollback": "knex migrate:rollback --knexfile src/config/knexfile.ts",
    "db:seed": "knex seed:run --knexfile src/config/knexfile.ts",
    "dev:prod": "cross-env NODE_ENV=production npm run db:migrate && cross-env NODE_ENV=production ts-node-dev --respawn --transpile-only src/index.ts",
    "db:clone:dev": "bash scripts/clone-db.sh"
  },
  "dependencies": {
    "@types/express-rate-limit": "^5.1.3",
    "@types/json2csv": "^5.0.7",
    "@types/xlsx": "^0.0.35",
    "agno": "^1.0.0",
    "apify-client": "^2.8.1",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "mvola-merchant-pay": "file:mvola-sdk",
    "nanoid": "^3.3.6",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "pg-connection-string": "^2.6.2",
    "stripe": "^20.3.1",
    "uuid": "^10.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/jszip": "^3.4.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
